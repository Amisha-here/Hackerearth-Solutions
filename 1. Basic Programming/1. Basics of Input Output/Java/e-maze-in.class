import java.util.*;
    class emaze
    {
        public static void main(String Args[])
        {
        Scanner in = new Scanner(System.in);
        String s= in.next();
        int x=0,y=0;
        for(int i=0;i<(s.length());i++)
        {
            char t =s.charAt(i);
            if(t=='L')
                x-=1;
            else if(t=='R')
                x+=1;
            else if(t=='U')
                y+=1;
            else 
                y-=1;
        }
        System.out.println(x+ " "+ y);
    }
}
